# Astria Vars (AI service)
ASTRIA_API_KEY=your_astria_api_key_here
ASTRIA_API_DOMAIN=https://api.astria.ai
NODE_ENV=development
ASTRIA_TEST_MODE=true

# Pack Configuration
PACK_QUERY_TYPE=gallery

# Webhook Secret
APP_WEBHOOK_SECRET=your_webhook_secret_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Vercel Blob Configuration
GOSTUDIO_WEB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Resend Configuration
RESEND_API_KEY=your_resend_api_key_here

# PayPal Configuration
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_client_id_here
CLIENT_SECRET=your_paypal_client_secret_here

# Deployment Configuration
DEPLOYMENT_PROVIDER=vercel
VERCEL_URL=your_vercel_url_here
NEXT_PUBLIC_DOMAIN=your_domain_here

# Analytics Configuration
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com
